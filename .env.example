# Template des variables d'environnement pour le Bot Discord Thugz Life RP
# Copiez ce fichier en .env et remplissez avec vos vraies valeurs

# Discord Bot Configuration (OBLIGATOIRE)
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_GUILD_ID=your_discord_server_id_here

# Supabase Database (OBLIGATOIRE)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Health Monitoring (RECOMMANDÉ)
ENABLE_HEALTH_MONITOR=true
HEALTH_PORT=8000

# Twitter Integration (OPTIONNEL)
TWITTER_CONSUMER_KEY=your_twitter_consumer_key
TWITTER_CONSUMER_SECRET=your_twitter_consumer_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret

# Development Settings (OPTIONNEL)
DEBUG=false
LOG_LEVEL=INFO

# Render.com specific (AUTO-SET)
# PORT=10000  # Automatiquement défini par Render
# PYTHON_VERSION=3.12.0
